<p id="notice"><%= notice %></p>

<h1>Infos</h1>

<table class="table table-success table-striped">
  <thead>
      <tr>
      <th>User Name</th>
      <th>Email</th>
      <th>Website Name</th>
      <th>Website Password</th>
      <th colspan="3"></th>

      </tr>
  </thead>

  <tbody>
    <% @infos.each do |info| %>
      <tr>
       <td><%= info.user_name %></td>
       <td><%= info.email %></td>
       <td><%= info.website_name %></td>
       <td><%= info.password %></td>
       <td><%= link_to 'Show', info %></td>
       <td><%= link_to 'Edit', edit_info_path(info) %></td>
       <td><%= link_to 'Destroy', info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info', new_info_path %>
